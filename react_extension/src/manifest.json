{
  "manifest_version": 3,
  "name": "ReactJs Extension",
  "version": "1.0.0",
  "description": "ReactJs extension helps you track your favorite TV shows.",
  "icons": {
    "16": "assets/icon.png",
    "48": "assets/icon.png",
    "128": "assets/icon.png"
  },
  "permissions": [
    "contextMenus",
    "search",
    "tabs",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/icon.png",
      "48": "assets/icon.png",
      "128": "assets/icon.png"
    }
  }
}
